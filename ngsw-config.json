{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": ["/favicon.ico", "/index.html", "/*.css", "/*.js"]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api",
      "urls": ["https://api.themoviedb.org/**", "https://image.tmdb.org/**"],
      "cacheConfig": {
        "strategy": "performance",
        "maxSize": 15,
        "maxAge": "1h",
        "timeout": "4s"
      }
    }
  ]
}
